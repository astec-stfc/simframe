io_path{logfile = log.txt}
lattice{
quadrupole{
position{rho=0.4, psi=0.0, marker=screen1a}
properties{strength=0.0, alpha=0, horizontal_offset=0,vertical_offset=0}
position{rho=0.400001, psi=0.0, marker=screen1b}
}
}
particles{
reference_momentum=reference_particle
reference_point_x=0.0
reference_point_y=0.0
reference_point_phi=0.0
format=astra
array=#file{name=BEGIN.astra}
}
forces{
type=projected
shape=ellipsoid
sigma_long=relative
relative_long=0.1
}
track_step{
precondition=yes
iterative=2
error_per_ct=0.001
error_weight_momentum=0.1
ct_step_min=0.002
ct_step_max=0.2
ct_step_first=0.01
increase_factor=2
arc_factor=0.3
duty_steps=yes
}
tracker{
end_time_marker=screen1a
end_time_shift_c0=0.0
}
monitor{
format=fmt2
name=END.fmt2
}
